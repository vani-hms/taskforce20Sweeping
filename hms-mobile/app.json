{
  "expo": {
    "name": "hms-mobile",
    "slug": "hms-mobile",
    "version": "1.0.0",
    "scheme": "hms-mobile",
    "orientation": "portrait",
    "platforms": [
      "android"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-secure-store",
      "expo-router"
    ],
    "android": {
      "package": "com.apricity.taskforce20",
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY_HERE"
        }
      }
    },
    "experiments": {
      "turboModules": false
    }
  }
}